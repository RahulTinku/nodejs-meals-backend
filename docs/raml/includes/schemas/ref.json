{
  "type": "object",
  "properties": {
    "phone": {
      "type": "string",
      "pattern": "^([+][0-9]{1,3}[ ]{1}[0-9]{9,10})$"
    },
    "password": {
      "type": "string",
      "minLength": 10
    },
    "firstName": {
      "type": "string",
      "pattern": "^([a-zA-Z]+)$"
    },
    "lastName": {
      "type": "string",
      "pattern": "^([a-zA-Z]+)$"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "expectedCalories": {
      "type": "number"
    },
    "roles": {
      "type": "string"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["firstName", "lastName", "email", "password"]
}